<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Sokoban_server</servlet-name>
        <servlet-class>com.sokoban_craze.sokoserver.Sokoban_Servlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>Sokoban_server</servlet-name>
		<url-pattern>/sokoban_server</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
